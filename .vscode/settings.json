{
  /* vscode - explorer */
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "README.md": "*.config.js,.npmrc,.gitignore,package.json,package-lock.json"
  },

  /* extensions - Vite */
  "vite.autoStart": false,
  "vite.browserType": "system",
  "vite.buildCommand": "npm run docs:build",
  "vite.devCommand": "npm run docs:dev",
  "vite.port": 5173,
}
